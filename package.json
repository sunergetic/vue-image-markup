{
  "name": "vue-image-markup",
  "version": "4.0.0",
  "description": "vue-image-markup will provide you to edit uploaded image easily and save it.",
  "main": "src/Editor.vue",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunergetic/vue-image-markup.git"
  },
  "keywords": [
    "image",
    "editor",
    "image editor",
    "image-editor",
    "markup",
    "image markup",
    "image-markup",
    "editor markup",
    "vuejs",
    "vue",
    "sunergetic"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-use-before-define": "off",
      "no-used": "off",
      "no-unused-vars": "off",
      "no-debugger": "off",
      "semi": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "license": "MIT",
  "bugs": {
    "url": "https://https://github.com/sunergetic/vue-image-markup/issues"
  },
  "homepage": "https://https://github.com/sunergetic/vue-image-markup#readme",
  "dependencies": {
    "fabric": "^5.3.0"
  }
}
